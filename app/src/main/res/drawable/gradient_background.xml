<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#F8FAFC" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:type="radial"
                android:gradientRadius="500dp"
                android:startColor="#E0F2FE"
                android:endColor="#00FFFFFF"
                android:centerX="0.2"
                android:centerY="0.1" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:type="linear"
                android:angle="315"
                android:startColor="#F1F5F9"
                android:endColor="#00FFFFFF" />
        </shape>
    </item>
</layer-list>